package clase4;

public class ComprobacionEdad {
	
	private String edad;
	
	public ComprobacionEdad(String edad){
		this.edad = edad;
	}
	
	public boolean comprobar(){
	boolean valido = false;
	
		for (char ch : edad.toCharArray()) {
			if (!Character.isDigit(ch)) {
				valido = false;
				break;
			}
	}

}
