package decorators;

public class Encrypter extends AbstractDecorator{

	public Encrypter(Decorator decorator) {
		super(decorator);
	}

	@Override
	public void Execute() {
		
	for(char c: mensaje){
			if (Character.isLetter(c) || Character.isDigit(c)) {
				int value = c;
				return (char) (c + 1);

			}}

		}
		
	}
}
