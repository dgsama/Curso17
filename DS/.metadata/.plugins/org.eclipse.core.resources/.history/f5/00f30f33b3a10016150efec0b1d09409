package main;

import java.io.*;

import outputs.*;

public class FileSystem {

	public void copy(String name, Output output) {
		try {
			FileReader reader = new FileReader(name);

			int c;
			int previous;
			while ((c = reader.read()) != -1)
				if (Character.compare((char) c, '\r') == 0) {
					c = '\n';
				}
			if (previous == 32 && c == 32) {
				output.send((char) c);
			}
			previous = c;

			reader.close();
			output.close();

		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
