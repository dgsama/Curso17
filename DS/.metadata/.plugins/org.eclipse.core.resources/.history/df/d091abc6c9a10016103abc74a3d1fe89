package decorators;

public class Encrypter extends AbstractDecorator {

	private

	public Encrypter(Decorator decorator, String mensaje) {
		super(decorator, mensaje);
	}

	@Override
	public void Execute() {

		for (char c : mensaje) {
			if (Character.isLetter(c) || Character.isDigit(c)) {
				int value = c;
				return (char) (c + 1);

			}
		}

	}

}
