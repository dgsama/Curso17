package campos;

import clase4.CampoAbstracto;

public class CampoCodigoProducto extends CampoAbstracto {

	public CampoCodigoProducto(String etiqueta) {
		super(etiqueta);
	}

	@Override
	public boolean comprobar() {
		if (getString().length() != 4) {
			return false;
		} else {
			return true;
		}
	}

}
