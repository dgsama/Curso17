package decorators;

public class Normalizer extends AbstractDecorator {

	public Normalizer(Decorator decorator, String mensaje) {
		super(decorator, mensaje);
	}

	@Override
	public void Execute() {
		// TODO Auto-generated method stub

	}
	

	public boolean normalize(char c) {
		if (c == '\r') {
			return true;
		}
		return false;
	}

}
