package clase4;

import java.util.*;

public class Formulario {
	public void addCampo(CampoAbstracto campo) {
		campos.add(campo);
	}

	public void PideDatos() {
		for (CampoAbstracto campo : campos) {
			campo.pideDato();
			System.out.println(campo.getString());
		}
	}

	private List<CampoAbstracto> campos = new ArrayList<CampoAbstracto>();
}
