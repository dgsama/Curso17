package decorators;

public class Spaces extends AbstractDecorator {

	public Spaces(Decorator decorator) {
		super(decorator);
	}

	@Override
	public String format(String c) {
		char aux = c;
		if (decorator != null) {
			if (aux == ' ' && previous == ' ') {
				return 0;
			}
		}
		return aux;
	}

}
